<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Reject_Quotes_Cant_Be_Edited</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
Rejected__c,
NOT( $Profile.Name=&quot;System Administrator&quot;)
)</errorConditionFormula>
    <errorMessage>Rejected quotes cannot be edited. Please create a new quote and submit for approval.</errorMessage>
</ValidationRule>
