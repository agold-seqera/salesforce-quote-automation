<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Effective_Start_Date__c</fullName>
    <description>3-level hierarchy: QLI Service Date → Quote Order Date → Opportunity Contract Date</description>
    <formula>IF(NOT(ISBLANK(Service_Start_Date__c)), Service_Start_Date__c, IF(NOT(ISBLANK(Quote.Order_Start_Date__c)), Quote.Order_Start_Date__c, Quote.Opportunity.Contract_Start_Date__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Effective Start Date</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Date</type>
</CustomField>
